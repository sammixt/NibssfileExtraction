<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="requiredCell" type="NibssFileExtraction.RequiredCellSection,NibssFileExtraction" />
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

  <appSettings>

    <add key="LOG_FILE_SIZE" value="5"/>
    <add key="LOG_PATH" value="C:\Users\esc\source\repos\"/>
    <add key="SuccessfulJsonFiles" value="\\Mac\Home\Desktop\files.json"/>
    <add key="UnsuccessfulJsonFiles" value="\\Mac\Home\Desktop\failedfiles.json"/>
    <add key="SuccessfulTable" value="NIBSS"/>
    <add key="UnsuccessfulTable" value="NIBSS_FAILED"/>
    <add key="timeout" value="600"/>
    <add key="dbip" value="ESC2581\SQLEXPRESS"/>
    <add key="db" value="RPA"/>
    <add key="downloadResultPath" value="C:\Users\esc\source\repos\result.txt"/>
    <add key="messagePath" value="C:\Users\esc\source\repos\message.txt"/>
  </appSettings>
  <requiredCell>
    <cells>
      <add cellName="F1" columnName="S/N" />
      <add cellName="F2" columnName="CHANNEL" />
      <add cellName="F3" columnName="SESSION ID" />
      <add cellName="F4" columnName="TRANSACTION TYPE" />
      <add cellName="F5" columnName="RESPONSE" />
      <add cellName="F6" columnName="AMOUNT" />
      <add cellName="F7" columnName="TRANSACTION TIME" />
      <add cellName="F8" columnName="ORIGINATOR / BILLER" />
      <add cellName="F9" columnName="DESTINATION ACCOUNT NAME" />
      <add cellName="F10" columnName="DESTINATION ACCOUNT NO" />
      <add cellName="F11" columnName="NARRATION" />
      <add cellName="F12" columnName="PAYMENT REFERENCE" />
      <add cellName="F13" columnName="LAST 12 DIGITS OF SESSION_ID" />
    </cells>
  </requiredCell>
  
</configuration>